{
  "name": "vue3-typescript-chrome-extension",
  "version": "1.0.0",
  "productName": "Vue3 + Typescript (MV3)",
  "description": "Chrome extension template with Vue 3 + Vuex 4 + Vue Router 4 + Typescript 4.2 + SCSS + Webpack 5.",
  "scripts": {
    "build": "cross-env NODE_ENV=\"production\" webpack",
    "build:dev": "cross-env NODE_ENV=\"development\" webpack",
    "build:dev:mock": "cross-env REPO_ENV=\"mock\" npm run build:dev",
    "watch": "cross-env NODE_ENV=\"development\" webpack --watch",
    "watch:mock": "cross-env REPO_ENV=\"mock\" npm run watch",
    "zip": "node scripts/zip_dist.js",
    "clean": "rimraf dist",
    "lint": "eslint ./src",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/taturou/vue3-typescript-chrome-extension"
  },
  "keywords": [
    "Chrome",
    "Extension",
    "Vue3",
    "Vuex4",
    "Vuex-Router4",
    "Typescript4.2",
    "Pug",
    "sass",
    "scss",
    "Webpack5"
  ],
  "author": "taturou <taturou@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taturou/vue3-typescript-chrome-extension/issues",
    "email": "taturou@gmail.com"
  },
  "homepage": "https://github.com/taturou/vue3-typescript-chrome-extension",
  "devDependencies": {
    "@prettier/plugin-pug": "^1.13.5",
    "@types/chrome": "^0.0.133",
    "@types/jquery": "^3.5.5",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vue/compiler-sfc": "^3.0.10",
    "@vue/eslint-config-typescript": "^7.0.0",
    "archiver": "^5.3.0",
    "comment-json": "^4.1.0",
    "copy-webpack-plugin": "^8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^7.8.0",
    "eslint-webpack-plugin": "^2.5.3",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "pug": "^3.0.2",
